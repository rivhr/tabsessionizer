{
  "content_scripts": [
      {
      "matches": ["<all_urls>"],
      "js": ["index.js"]
      }
  ],
  "background": {
      "scripts": ["index.js"]
  },

  "browser_action": {
    "browser_style": true,
    "default_title": "sessionizer",
    "default_popup": "index.html"
  },
  "description": "blabla",
  "homepage_url": "https://github.com/rivhr/tabsessionizer",
  "manifest_version": 2,
  "name": "tab sessionizer",
  "permissions": [
      "<all_urls>",
      "tabs"
  ],
  "version": "1.0"
}
